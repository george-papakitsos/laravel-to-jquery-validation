{
    "name": "gpapakitsos/laravel-to-jquery-validation",
    "description": "Converts laravel validation rules to jQuery validation rules",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "George Papakitsos",
            "email": "papakitsos_george@yahoo.gr",
            "homepage": "https://papakitsos.gr"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": ">=5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7"
    },
    "autoload": {
        "psr-4": {
            "GPapakitsos\\LaravelToJqueryValidation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GPapakitsos\\LaravelToJqueryValidation\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
